
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import jellium_scf

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5
}

set {
  basis sto-3g
  n_grid_points         30
  n_electrons           30
  n_basis_functions     60
  time_step             0.1
  time_length           10
}

energy('jellium_scf')
